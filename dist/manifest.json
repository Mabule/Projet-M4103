{
    "manifest_version": 2,
    "name": "Weather.js",
    "description": "Application fournissant la météo dépendamment d'une localisation",
    "version": "1.0",
    "permissions": [
                  "http://*/*",
                  "https://*/*"
    ],
    "content_security_policy": "script-src 'self' https://kit.fontawesome.com/e4f326d654.js; object-src 'self'",
    "browser_action": {
      "default_icon": "./logo.png",
      "default_popup": "./pop.html"
    }
}
  